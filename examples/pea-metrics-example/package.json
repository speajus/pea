{
  "name": "@speajus/pea-metrics-example",
  "type": "module",
  "version": "1.0.0",
  "description": "A simple example on how to use @speajus/pea-metrics in your app",
  "scripts": {
    "prometheus": "docker run -p 9090:9090  -v ./prometheus.yml:/etc/prometheus/prometheus.yml prom/prometheus",
    "start": "tsx ./src/server.ts",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "prometheus",
    "@speajus/pea",
    "metrics",
    "examples"
  ],
  "author": "Justin Spears <speajus@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@speajus/pea": "link:../../packages/pea",
    "@speajus/pea-metrics": "link:../../packages/pea-metrics",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "tsx": "^4.19.2"
  }
}